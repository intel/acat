      <Widget name="B44"/>
      <Widget name="B45" onSelect="actuate(@SelectedWidget); transition(TopLevelRotation)"/>
      <Widget name="B46" onSelect="actuate(@SelectedWidget); transition(TopLevelRotation)"/>
      <Widget name="B47" onSelect="actuate(@SelectedWidget); transition(TopLevelRotation)"/>
