<?xml version="1.0" ?>
<!DOCTYPE ACAT [
	<!ENTITY usebold "false">
	<!ENTITY functionButtonFontName "acat font 1">
	<!ENTITY functionButtonFontSize "35">
	<!ENTITY characterButtonFontName "montserrat semibold">
	<!ENTITY characterButtonFontSize "30">
	<!ENTITY predTextFontName "montserrat">
	<!ENTITY predTextButtonFontSize "14">
	<!ENTITY buttonFontName "Arial">

]>

<ACAT>
	
  <WidgetAttributes>

	<WidgetAttribute name="B1" label="" value="" fontname="&characterButtonFontName;"    fontsize="&characterButtonFontSize;"  bold="&usebold;"  extendedAttributes="gazeRepeatEnable=true;"/>
	<WidgetAttribute name="B2" label="" value="" fontname="&characterButtonFontName;"    fontsize="&characterButtonFontSize;"  bold="&usebold;"  extendedAttributes="gazeRepeatEnable=true;"/>
	<WidgetAttribute name="B3" label="" value="" fontname="&characterButtonFontName;"    fontsize="&characterButtonFontSize;"  bold="&usebold;"  extendedAttributes="gazeRepeatEnable=true;"/>
	<WidgetAttribute name="B4" label="" value="" fontname="&characterButtonFontName;"    fontsize="&characterButtonFontSize;"  bold="&usebold;"  extendedAttributes="gazeRepeatEnable=true;"/>
	<WidgetAttribute name="B5" label="" value="" fontname="&characterButtonFontName;"    fontsize="&characterButtonFontSize;"  bold="&usebold;"  extendedAttributes="gazeRepeatEnable=true;"/>

	<WidgetAttribute name="B6" label="ABC" value="@CmdKeyboardMain" fontname="&characterButtonFontName;" italic="true" fontsize="35"  bold="true" />
	<WidgetAttribute name="B7" label="" value="" fontname="&characterButtonFontName;"    fontsize="&characterButtonFontSize;"  bold="&usebold;"  extendedAttributes="gazeRepeatEnable=true;"/>
	<WidgetAttribute name="B8" label="Yes" value="@CmdTTSYes" fontname="&characterButtonFontName;"    fontsize="&characterButtonFontSize;"  bold="&usebold;"  extendedAttributes="gazeRepeatEnable=true;"/>
	<WidgetAttribute name="B9" label="" value="" fontname="&characterButtonFontName;"    fontsize="&characterButtonFontSize;"  bold="&usebold;"  extendedAttributes="gazeRepeatEnable=true;"/>
	<WidgetAttribute name="B10" label="No" value="@CmdTTSNo" fontname="&characterButtonFontName;"    fontsize="&characterButtonFontSize;"  bold="&usebold;"  extendedAttributes="gazeRepeatEnable=true;"/>

	<WidgetAttribute name="B11" label="" value="" fontname="&functionButtonFontName;"    fontsize="&characterButtonFontSize;"  bold="&usebold;"  extendedAttributes="gazeRepeatEnable=true;"/>
    <WidgetAttribute name="B12" label="" value="" fontname="&functionButtonFontName;"    fontsize="&characterButtonFontSize;"  bold="&usebold;"  extendedAttributes="gazeRepeatEnable=true;"/>
	<WidgetAttribute name="B13" label="" value="" fontname="&characterButtonFontName;"    fontsize="&characterButtonFontSize;"  bold="&usebold;"  extendedAttributes="gazeRepeatEnable=true;"/>
	<WidgetAttribute name="B14" label="" value="" fontname="&characterButtonFontName;"    fontsize="&characterButtonFontSize;"  bold="&usebold;"  extendedAttributes="gazeRepeatEnable=true;"/>
	<WidgetAttribute name="B15" label="" value="" fontname="&characterButtonFontName;"    fontsize="&characterButtonFontSize;"  bold="&usebold;"  extendedAttributes="gazeRepeatEnable=true;"/>

  </WidgetAttributes>

  <Layout>
	  <Widget class="WinControlWidget"  name="Keyboard">
		  <Widget class="ScannerButton" name="B6" />
		  <Widget class="ScannerButton" name="B7" />
		  <Widget class="ScannerButton" name="B8" />
		  <Widget class="ScannerButton" name="B9" />
		  <Widget class="ScannerButton" name="B10" />
	  </Widget>

	  <Layouts name="ProgressBarsBoxMapping">
		  <Widget name="PBMain1" tagID="1"/>
	  </Layouts>

	  <Layouts name="ProgressBarsMapping">
		  <Widget name="PB6" tagID="1"/>
		  <Widget name="PB7" tagID="2"/>
		  <Widget name="PB8" tagID="3"/>
		  <Widget name="PB9" tagID="4"/>
		  <Widget name="PB10" tagID="5"/>
	  </Layouts>
	  <Layouts name="KeyboardBoxMapping">
		<Widget name="B6" tagID="1"/>
		<Widget name="B7" tagID="2"/>
		<Widget name="B8" tagID="3"/>
		<Widget name="B9" tagID="4"/>
		<Widget name="B10" tagID="5"/>
	  </Layouts>
	  <Layouts name="KeyboardType">
		  <Widget name="Menus"/>
	  </Layouts>
  </Layout>

  <Animations>
    <Animation name="TopLevelRotation"
               start="true"
               iterations="@GridScanIterations">
    </Animation>
	<Animation name="KeyboardSequences">
		<Animation name="Column">
			<Widget name="B6" tagID="1"/>
		</Animation>
		<Animation name="Column">
			<Widget name="B7" tagID="2"/>
		</Animation>
		<Animation name="Column">
			<Widget name="B8" tagID="3"/>
		</Animation>
		<Animation name="Column">
			<Widget name="B9" tagID="4"/>
		</Animation>
		<Animation name="Column">
			<Widget name="B10" tagID="5"/>
		</Animation>
	</Animation>
   
	  <Animation name="ButtonPressed"
		   onEnter=""
		   iterations="-1"
		   onEnd="transition(TopLevelRotation)">
		  <Widget name="@SelectedWidget" firstPauseTime="@FirstRepeatTime" onSelect="actuate(@SelectedWidget);transition(ButtonPressed)" />
	  </Animation>
	  
	  

    <Animation name="FunctionRotation"
                iterations="@RowScanIterations"
               firstPauseTime="@FirstPauseTime"
               onEnd="transition(TopLevelRotation)">
      
    </Animation>

    <Animation name="KeyboardRotation"
                iterations="@RowScanIterations"
               firstPauseTime="@FirstPauseTime"
               onEnd="transition(TopLevelRotation)">

    </Animation>

    <Animation name="SpeechRotation"
                iterations="@RowScanIterations"
               firstPauseTime="@FirstPauseTime"
               onEnd="transition(TopLevelRotation)">

    </Animation>


  </Animations>
</ACAT>
