  <Animations>
    <Animation name="TopLevelRotation"
               start="true"
               onEnter="highlight(PredictionWordList, false); "
               iterations="@HalfScanIterations">
      <Widget name="Box1" onSelect="transition(RowRotationBox1)"/>
      <Widget name="Box2" onSelect="transition(RowRotationBox2)"/>
    </Animation>

    <Animation name="RowRotationBox1"
               iterations="@RowScanIterations"
               hesitateTime="@HesitateTime"
               onEnd="transition(TopLevelRotation)">
      <Widget name="Row1" onSelect="transition(ButtonRotationBox1)"/>
      <Widget name="Row2" onSelect="transition(ButtonRotationBox1)"/>
      <Widget name="Row3" onSelect="transition(ButtonRotationBox1)"/>
      <Widget name="Row4" onSelect="transition(ButtonRotationBox1)"/>
    </Animation>

    <Animation name="ButtonRotationBox1"
               iterations="@ColumnScanIterations"
               onEnd="transition(RowRotationBox1)"
               hesitateTime="@HesitateTime"
              onSelect="actuate(@SelectedWidget); transition(TopLevelRotation)">
      <Widget name="@SelectedWidget/*"/>
    </Animation>

    <Animation name="RowRotationBox2"
               onBack="transition(TopLevelRotation)"
               hesitateTime="@HesitateTime"
               iterations="@RowScanIterations"
               onEnd="transition(TopLevelRotation)">
      <Widget name="Row5" onSelect="transition(ButtonRotationRow5Box2)" />
      <Widget name="Row6" onSelect="transition(ButtonRotationRow6Box2)"/>
      <Widget name="Row7" onSelect="transition(ButtonRotationRow7Box2)"/>
      <Widget name="Row8" onSelect="transition(ButtonRotationRow8Box2)"/>
    </Animation>

    <Animation name="ButtonRotationRow5Box2"
               iterations="@ColumnScanIterations"
               onEnd="transition(RowRotationBox2)"
               hesitateTime="@HesitateTime">
      <Widget name="B29" onHighlightOn="highlight(PWLItem1, true)" onHighlightOff="highlight(PWLItem1, false)" onSelect="actuate(PWLItem1); select(PWLItem1); highlight(PWLItem1, false);transition(TopLevelRotation)" />
      <Widget name="B30" onHighlightOn="highlight(PWLItem2, true)" onHighlightOff="highlight(PWLItem2, false)" onSelect="actuate(PWLItem2); select(PWLItem2); highlight(PWLItem2, false);transition(TopLevelRotation)"/>
      <Widget name="B31" onHighlightOn="highlight(PWLItem3, true)" onHighlightOff="highlight(PWLItem3, false)" onSelect="actuate(PWLItem3); select(PWLItem3); highlight(PWLItem3, false);transition(TopLevelRotation)"/>
      <Widget name="B32" onHighlightOn="highlight(PWLItem4, true)" onHighlightOff="highlight(PWLItem4, false)" onSelect="actuate(PWLItem4); select(PWLItem4); highlight(PWLItem4, false);transition(TopLevelRotation)"/>
      <Widget name="B33" onHighlightOn="highlight(PWLItem5, true)" onHighlightOff="highlight(PWLItem5, false)" onSelect="actuate(PWLItem5); select(PWLItem5); highlight(PWLItem5, false);transition(TopLevelRotation)"/>
      <Widget name="B34" onHighlightOn="highlight(PWLItem6, true)" onHighlightOff="highlight(PWLItem6, false)" onSelect="actuate(PWLItem6); select(PWLItem6); highlight(PWLItem6, false);transition(TopLevelRotation)"/>
      <Widget name="B35" onHighlightOn="highlight(PWLItem6, false)" onSelect="actuate(B35)"/>
    </Animation>

    <Animation name="ButtonRotationRow6Box2"
                   iterations="@ColumnScanIterations"
                   onEnd="transition(RowRotationBox2)"
                   hesitateTime="@HesitateTime">
      <Widget name="B36" onHighlightOn="highlight(PWLItem7, true)" onHighlightOff="highlight(PWLItem7, false)" onSelect="actuate(PWLItem7); select(PWLItem7); highlight(PWLItem7, false);transition(TopLevelRotation)"/>
      <Widget name="B37" onHighlightOn="highlight(PWLItem8, true)" onHighlightOff="highlight(PWLItem8, false)" onSelect="actuate(PWLItem8); select(PWLItem8); highlight(PWLItem8, false);transition(TopLevelRotation)"/>
      <Widget name="B38" onHighlightOn="highlight(PWLItem9, true)" onHighlightOff="highlight(PWLItem9, false)" onSelect="actuate(PWLItem9); select(PWLItem9); highlight(PWLItem9, false);transition(TopLevelRotation)"/>
      <Widget name="B39" onHighlightOn="highlight(PWLItem10, true)" onHighlightOff="highlight(PWLItem10, false)" onSelect="actuate(PWLItem10); select(PWLItem10); highlight(PWLItem10, false);transition(TopLevelRotation)"/>
      <Widget name="B40" onSelect="actuate(@SelectedWidget); transition(TopLevelRotation)"/>
      <Widget name="B41" onSelect="actuate(@SelectedWidget); transition(TopLevelRotation)"/>
      <Widget name="B42" onSelect="actuate(@SelectedWidget); transition(TopLevelRotation)"/>
    </Animation>

