      <Widget name="B52" onSelect="actuate(@SelectedWidget); transition(TopLevelRotation)"/>
      <Widget name="B53" onSelect="actuate(@SelectedWidget); transition(TopLevelRotation)"/>
      <Widget name="B54" onSelect="actuate(@SelectedWidget); transition(TopLevelRotation)"/>
      <Widget name="B55" onSelect="actuate(@SelectedWidget); transition(TopLevelRotation)"/>
